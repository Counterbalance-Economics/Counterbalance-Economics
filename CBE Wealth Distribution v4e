;; (c) Peter Malliaros, 2021
;; Impact to Inequality of introducing the Counterbalance Economics System into the Wealth Distribution Model

breed [ employees employee ]   ;; defines agentset and agent

globals        ;; general parameters accessible by all agents from across the world
[
  max-grain    ;; maximum amount any patch can hold
  lorenz-points ;; variable required to calculate the lorenz curve
  gini-index-reserve ;; resets the value to allow recalculation
  initial-bottom-50-inc ;;


]

patches-own
[
  grain-here      ; the current amount of grain on this patch
  max-grain-here  ; the maximum amount of grain this patch can hold
 ]

employees-own
[
  age              ; how old an employee is
  wealth           ; the total amount of grain an employee has collected
  income           ; the amount of grain an employee has harvested at each time interval
  life-expectancy  ; maximum age that an employee can reach
  metabolism       ; how much grain an employee eats each time
  vision           ; how many patches ahead an employee can see
  Tax-Collected    ; Amount of income reduced by tax
  Welfare          ; Amount received as welfare payment
]

;;;
;;; SETUP AND HELPERS
;;;

to setup
  clear-all
  ;;
  set max-grain Country-AWE ;; sets up the maximum amount of grain that a patch can hold
  setup-patches    ;; call procedures to set up patches
  setup-employees  ;; call procedures to set up employees
  update-lorenz-and-gini ;; updates the lorenz and gini outputs
  reset-ticks
end

;; set up the initial amounts of grain each patch has
to setup-patches
  ;; give some patches the highest amount of grain possible --
  ;; these patches are the "best land"
  ask patches
    [ set max-grain-here 0
      if (random-float 100.0) <= percent-best-land
        [ set max-grain-here max-grain
          set grain-here max-grain-here ] ]
  ;; spread that grain around the window a little and put a little back
  ;; into the patches that are the "best land" found above
  repeat 5
    [ ask patches with [max-grain-here != 0]
        [ set grain-here max-grain-here ]
      diffuse grain-here 0.25 ]
  repeat 10
    [ diffuse grain-here 0.25 ]          ;; spread the grain around some more
  ask patches
    [ set grain-here floor grain-here    ;; round grain levels to whole numbers
      set max-grain-here grain-here      ;; initial grain level is also maximum
      recolor-patch ]
end

to recolor-patch  ;; patch procedure -- use color to indicate grain level
  set pcolor scale-color yellow grain-here 0 max-grain
end

;; set up the initial values for the turtle variables
to setup-employees
  set-default-shape employees "person"
  create-employees num-people
    [ move-to one-of patches  ;; put turtles on patch centers
    set size 1.5  ;; easier to see
      set-initial-employee-vars
      set age random life-expectancy ]
 recolor-employees
end

to set-initial-employee-vars

  set age 0
  face one-of neighbors4
  set life-expectancy life-expectancy-min +
                        random (life-expectancy-max - life-expectancy-min + 1)
  set metabolism 1 + random metabolism-max
  set income metabolism + random Country-AWE
  set wealth (wealth + 1)
  set vision 1 + random max-vision

end

;; Set the class of the turtles -- if a turtle has less than a third
;; the wealth of the richest turtle, color it red.  If between one
;; and two thirds, color it green.  If over two thirds, color it blue.
;; setup recolor based on chooser
to recolor-employees
  let max-income max [income] of employees

 ;; when chosen, this component returns this simulation back to its original setting to provide a baseline for comparison
  if ( Economic-System = "Original-Model" ) [ ask employees [ ifelse (income <= max-income / 2)
        [ set color red ]
    [ set color blue ] ] ]

  if ( Economic-System = "Counterbalance" ) [ ask employees [ ifelse income <= cbe-mean
        [ set color red ]
           [ set color blue ] ] ]

  if ( Economic-System = "Neoliberal-v-GovernmentRun" ) [ ask employees [ ifelse (income <= max-income / 2)
        [ set color red ]
    [ set color blue ] ] ]

economic-system-selector
end

to economic-system-selector
  let max-income max [income] of employees

    if ( Economic-System = "Original-Model" ) [ ask employees [ ifelse (income <= max-income / 2)
    [set Tax-Collected  0 set welfare 0 ]
      [set Tax-Collected 0 ] ]]

  if ( Economic-System = "Counterbalance" ) [ ask employees with [color = red][ ifelse income <= cbe-mean
     [set income (cbe-mean ) set Tax-Collected 0  set welfare 0 ]
      [set income income set Tax-Collected 0 set welfare 0 ]]]

  if ( Economic-System = "Counterbalance" ) [ ask employees with [color = blue][ ifelse income <= cbe-mean
  [set income (cbe-mean ) set Tax-Collected 0  set welfare 0 ]
    [set income (income - Tax-Collected)set welfare 0 ]]]

    ;; when chosen, this component allows you to vary the overall tax rate and the welfare paid to the reds
  if ( Economic-System = "Neoliberal-v-GovernmentRun" ) [ ask employees [ ifelse (income <= max-income / 2)
    [set income ((income + Welfare) - Tax-Collected) ]
       [set income (income - Tax-Collected) set welfare 0 ] ] ]

end

;;;
;;; GO AND HELPERS
;;;

to go ;; simulation begins

if ticks >= 500 [ stop ] ;; stops the simulation after 500 weeks (ticks)

  if ticks = 0 [ set initial-bottom-50-inc bottom-50-inc ]

  ask employees
    [ turn-towards-grain ]  ;; choose direction holding most grain within the employee's vision
  harvest ;; runs the harvest procedure
  ask employees
    [ move-eat-age-die ] ;; asks employees to move towards grain, eat the grain, age by one week and die if they don't have enough energy
  recolor-employees ;; employees change their color based on how much wealth they have

   ask patches [ grow-grain ]

  update-lorenz-and-gini ;; updates the lorenz and gini outputs
  tick
end

to grow-grain  ;; patch procedure
  ;; if a patch does not have it's maximum amount of grain, add
  ;; num-grain-grown to its grain amount
  if (grain-here < max-grain-here)
    [ set grain-here grain-here + income-growth-volume
      ;; if the new amount of grain on a patch is over its maximum
      ;; capacity, set it to its maximum
      if (grain-here > max-grain-here)
        [ set grain-here max-grain-here ]
      recolor-patch ]
end

;; determine the direction which is most profitable for each employee in
;; the surrounding patches within the employees' vision
to turn-towards-grain  ;; employee procedure
  set heading 0
  let best-direction 0
  let best-amount grain-ahead
  set heading 90
  if (grain-ahead > best-amount)
    [ set best-direction 90
      set best-amount grain-ahead ]
  set heading 180
  if (grain-ahead > best-amount)
    [ set best-direction 180
      set best-amount grain-ahead ]
  set heading 270
  if (grain-ahead > best-amount)
    [ set best-direction 270
      set best-amount grain-ahead ]
  set heading best-direction
end

to-report grain-ahead  ;; employee procedure
  let total 0
  let how-far 1
  repeat vision
    [ set total total + [grain-here] of patch-ahead how-far
      set how-far how-far + 1 ]
  report total
end

;; each employee harvests the grain on its patch.  if there are multiple
;; employees on a patch, divide the grain evenly among the employees
to harvest
  ; have employees harvest before any employee sets the patch to 0
  ask employees
    [ set income floor (income + (grain-here / (count employees-here) )) ]
  ;; set income income
  ;; set wealth wealth - income

  ;; now that the grain has been harvested, have the employees make the
  ;; patches which they are on have no grain
  ask employees
    [ set grain-here 0
      recolor-patch ]

end

to move-eat-age-die  ;; employee procedure
  fd 1

  set Tax-Collected (income * Tax-Collected-as-%-of-GDP)
  set Welfare (income * Welfare-Payment-to-Reds)

  ;; consume some grain according to metabolism
  set wealth (( income - metabolism) + wealth )
  ;; grow older
  set age (age + 1)
  ;; check for death conditions: if you have no grain or
  ;; you're older than the life expectancy or if some random factor
  ;; holds, then you "die" and are "reborn" (in fact, your variables
  ;; are just reset to new random values)
  if (wealth < 0) or (age >= life-expectancy)
    [ set-initial-employee-vars ]
end

;; this procedure recomputes the value of gini-index-reserve
;; and the points in lorenz-points for the Lorenz and Gini-Index plots
to update-lorenz-and-gini
  let sorted-income sort [income] of employees
  let total-income sum sorted-income
  let income-sum-so-far 0
  let index 0
  set gini-index-reserve 0
  set lorenz-points []

  ;; now actually plot the Lorenz curve -- along the way, we also
  ;; calculate the Gini index.
  ;; (see the Info tab for a description of the curve and measure)
  repeat num-people [
   set income-sum-so-far (income-sum-so-far + item index sorted-income)
    set lorenz-points lput ((income-sum-so-far / total-income) * 100) lorenz-points
    set index (index + 1)
    set gini-index-reserve
      gini-index-reserve +
      (index / num-people) -
      (income-sum-so-far / total-income)
      ]
end

to-report num-of-red
  report round count employees with [color = red]
end

to-report num-of-blue
  report round count employees with [color = blue]
end

to-report avg-inc-red
    report round sum [ income ] of employees with [color = red] / num-of-red
end

to-report avg-inc-blue
    report round sum [ income ] of employees with [color = blue] / num-of-blue
end

to-report avg-inc-total
  report (sum [income] of employees) / num-people
end

to-report red-wealth
  report ( sum [ wealth ] of employees with [color = red] ) / count employees with [color = red]
end

to-report blue-wealth
  report ( sum [ wealth ] of employees with [color = blue] ) / count employees with [color = blue]
end

to-report tot-income-world
  report round sum [income] of employees
end

;; to-report tot-wealth-world
  ;; report round sum [wealth] of employees
;; end

to-report cbe-mean
  report round ( top-50-inc + initial-bottom-50-inc ) / num-people
end

to-report top-50-inc
  report round sum [income] of employees with [ who > ( num-people / 2 - 1 ) ]
end

to-report bottom-50-inc
report round sum [income] of employees with [ who < num-people / 2 ]
end





